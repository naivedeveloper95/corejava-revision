public class GCDemo {

	public GCDemo() {
		System.out.println(this + " Created.");
	}

	public static void main(String[] args) {

		new GCDemo();
		new GCDemo();
		new GCDemo();
	}

	@Override
	protected void finalize() throws Throwable {
		System.out.println(this + " Finalized.");
//		super.finalize();
	}
}
